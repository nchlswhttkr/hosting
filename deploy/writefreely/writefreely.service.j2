[Unit]
Description=Writefreely
Documentation=https://writefreely.org/docs
After=network.target

[Service]
ExecStart=/home/write/writefreely/writefreely
ExecStartPre=/home/write/writefreely/writefreely db init
ExecStartPre=/home/write/writefreely/writefreely keys generate
ExecStartPre=/home/write/writefreely/writefreely db migrate
WorkingDirectory=/home/write/writefreely

[Install]
WantedBy=default.target
